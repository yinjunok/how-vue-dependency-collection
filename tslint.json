{
  "extends": [
    "tslint:latest",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    // override tslint-react rules here
    "ordered-imports": false,
    "ter-indent": [true, 2],
    "jsx-no-lambda": false,
    // "interface-name" : [true, "never-prefix"],
    "no-empty": false,
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-console": [true, "warning"]
  }
}
